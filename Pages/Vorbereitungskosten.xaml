<Page x:Class="WpfAppToolBar.Pages.Vorbereitungskosten"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfAppToolBar.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Vorbereitungskosten">

    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="45"/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="55"/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <StackPanel Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="2">
            <TextBox x:Name="txt_boxVorbereitungoben" TextWrapping="Wrap" Margin="10"/>
        </StackPanel>
        
        
        
        
        

        <Button Command="NavigationCommands.BrowseBack"  Padding="5"  Content="Back" Margin="10,10,0,0"  Height="25" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Grid>
    
</Page>
